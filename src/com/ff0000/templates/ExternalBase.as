package com.ff0000.templates  {		import flash.display.MovieClip;	import flash.events.Event;	import com.ff0000.utils.zwrap.*;	import com.ff0000.utils.zdate.ZDate;	import com.greensock.TweenLite;		public class ExternalBase extends MovieClip {				public var defaultClickTag:String;		public var zwrapClasses:ZWrapClasses;		public function ExternalBase() {			// constructor code			addEventListener( Event.ADDED_TO_STAGE, handleAddedToStage );		}		private function handleAddedToStage( $_e:Event ):void {			//prepareZWrap();			ZWrap.get().classes.make( 'ZDate', 'zdate' );			ZWrap.get().classes.zdate.makeTimeFrom( new Date());						ZWrap.get().classes.make( 'ZDate', 'newZDate' );			ZWrap.get().classes.newZDate.makeTimeFrom( 'March 6, 1976' );			trace( ZWrap.get().classes.zdate.toZDateString());			trace( ZWrap.get().classes.newZDate.toZDateString());						ZWrap.get().classes.make( 'TweenLite' ).to( rect, 0.3, { x: 300 });		}		/*		protected function prepareZWrap():void {			ZWrap.get().classes.addClass( ZDate );			ZWrap.get().classes.addClass( TweenLite );		}*/	}	}